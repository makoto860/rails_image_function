<h1>Photos#edit</h1>
<p>Find me in app/views/photos/edit.html.erb</p>
<h1>写真編集</h1>
<%= form_with model: @photo do |f| %>
<table>
  <tr>
    <th><%= f.label :image, "image" %></th>
    <td><%= f.file_field :image %></td>
  </tr>
</table>
<div>
  <ul>
    <li><%= f.submit "編集を完了する" %></li>
    <li><%= link_to "写真情報一覧に戻る", :photos %></li>
  </ul>
</div>
<% end %>